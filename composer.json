{
    "name": "marellocommerce/marello-bridge-api",
    "description": "Marello Bridge API client library",
    "homepage": "https://github.com/marellocommerce/marello-bridge-api.git",
    "type": "library",
    "license": [
        "OSL-3.0"
    ],
    "authors": [
        {
            "name": "Marello",
            "homepage": "http://www.marello.com"
        }
    ],
    "require": {
        "php": ">=5.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-4": {
            "Marello\\": "src/Marello"
        }
    },
    "autoload-dev": {
        "psr-4": {"Marello\\Tests\\": "tests/Marello"}
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}